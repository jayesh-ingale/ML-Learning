        "def fit_and_score(models, X_train,X_test,y_train,y_test):\n",
        "  np.random.seed(42)\n",
        "  scores={}\n",
        "  for name,model in models.items():\n",
        "    model.fit(X_train,y_train)\n",
        "    model_score=model.score(X_test,y_test)\n",
        "    scores[name]=model_score\n",
        "  return scores\n"
